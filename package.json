{
  "name": "shopify-cli-3.0",
  "version": "3.0.0",
  "author": "SPLIT Development",
  "scripts": {
    "start": "run-p -sr shopify:dev gulp:watch",
    "shopify:dev": "shopify theme dev -s kosmobox --theme-editor-sync",   
    "shopify:pull": "shopify theme pull -s kosmobox",
    "deploy": "run-s gulp:production && shopify theme push -s kosmobox",
    "deploy:new": "run-s gulp:production && shopify theme push --unpublished -s kosmobox",
    "gulp:watch": "gulp",
    "gulp:production": "gulp production"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "autoprefixer": "^10.4.21",
    "gulp": "^5.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^6.0.1",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "sass": "^1.91.0"
  },
  "license": "ISC"
}
